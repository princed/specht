{
  "name": "specht",
  "version": "1.1.0",
  "description": "Check links found in html or js files by pattern",
  "bin": "lib/cli.js",
  "main": "lib/runner.js",
  "license": "MIT",
  "author": {
    "name": "Eugene Datsky",
    "email": "eugene@datsky.ru"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "scripts": {
    "lint": "eslint src",
    "test": "ava test/**/*Spec.js --source=src/**/*.js --fail-fast",
    "build": "babel src --out-dir lib --source-maps",
    "prepublish": "npm run build"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/princed/specht.git"
  },
  "bugs": {
    "url": "https://github.com/princed/specht/issues"
  },
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "babel-traverse": "6.23.1",
    "babel-types": "6.23.0",
    "babylon": "6.16.1",
    "ignore-file": "1.1.2",
    "parse5": "3.0.2",
    "readdirp": "2.1.0",
    "teamcity-service-messages": "0.1.8",
    "through2": "2.0.3",
    "yargs": "7.0.2"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-preset-es2015": "^6.24.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.2",
    "eslint-plugin-import": "^2.2.0",
    "mock-fs": "^4.2.0",
    "sinon": "^2.1.0"
  }
}
